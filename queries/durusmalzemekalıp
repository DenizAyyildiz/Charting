	SELECT BREAKDOWNSTART,BREAKDOWNEND,B.MATERIAL,K.TOOLNUM,X.STEXT
	FROM IASPRDFAILURE F
	LEFT JOIN IASPRDCONF C ON
	C.CONFIRMATION = F.CONFIRMATION
	AND C.CONFIRMPOS = F.CONFIRMPOS
	LEFT JOIN IASPRDOPR B ON
	C.CONFIRMATION = B.CONFIRMATION
	AND C.CONFIRMPOS = F.CONFIRMPOS
	LEFT JOIN IASPRDRST K ON
	C.PRDORDER = K.PRDORDER
	LEFT JOIN IASPRD005X X
	ON F.FAILURECODE = X.FAILURECODE
	WHERE C.WORKCENTER = 'P-71'
	AND F.BREAKDOWNSTART > '20240714'

